<% provide(:title, 'Update Profile') %>
<h1>Update Profile</h1>

<div class="row">
  <div class="span6">
    <%= form_for @user, controller: 'users', action: 'update' do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save profile", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
